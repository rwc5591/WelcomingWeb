act-dom/client\";\nimport { BrowserRouter, Routes, Route} from \"react-router-dom\";\nimport { Footer, Header, About, Kids, Adults, Resources} from './containers'\nimport { Navbar, Home, Quiz } from './components'\n\nimport './App.css'\n\nconst App = () => {\n  return (\n    \n    <div className='App'>\n      <div className='gradient_bg'>\n        <Navbar />\n        {/**<Header /> */}\n      </div>\n      {/**<About />}\n      {/**<Kids />}\n  {<Adults/> */}\n    \n    \n      {/**<Footer /> */} \n      <BrowserRouter>\n      <Routes>\n        {/**<Route path = \"/\" element={<Navbar />}> </Route>*/}\n          <Route path = \"Home\" element = {<Home />} />\n          <Route path = \"About\" element = {<About />} />\n          <Route path = \"Kids\" element ={<Kids />} />\n          <Route path = \"Adults\" element ={<Adults />} />\n          <Route path = \"Resources\" element ={<Resources />} />\n          <Route path = \"GoPhish\" element ={<Quiz />} />\n\n        {/**</Route>*/}\n      </Routes>\n      </BrowserRouter>\n    </div>\n  )\n}\n\nexport default App","/Users/regchuhi/swe250/WelcomingWeb/src/containers/index.js",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/components/index.js",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/containers/resources/Resources.jsx",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/containers/header/Header.jsx",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/containers/footer/Footer.jsx",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/containers/about/About.jsx",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/containers/kids/Kids.jsx",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/components/home/Home.jsx",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/containers/adults/Adults.jsx",["103","104"],[],"import React, {useState}from 'react'\nimport './adults.css'\nimport {Resources} from '../../containers'\nimport Button from 'react-bootstrap/Button';\nimport Carousel from 'react-bootstrap/Carousel';\nimport Row from 'react-bootstrap/Row';\nimport Col from 'react-bootstrap/Col';\nimport Container from 'react-bootstrap/Container';\nimport adultResources from './resources/adultResources.json';\nimport Card from 'react-bootstrap/Card';\n\nconst Adults = () => {\n  return (\n    ImportantInfoCarousel()\n  )\n}\n\nfunction ImportantInfoCarousel(){\n  const [index, setIndex] = useState(0);\n\n  const handleSelect = (selectedIndex, e) => {\n    setIndex(selectedIndex);\n  }\n\n  const resources= adultResources.map((resources) => {\n    return(\n      <Card key ={resources.id}>\n        <Card.Body>\n        <Card.Title>{resources.name}</Card.Title>\n        <Card.Text>{resources.desc}</Card.Text>\n        <a className=\"btn-primary\"\n        href={resources.url}>\n          Go to Website\n        </a>\n        </Card.Body>\n      </Card>\n    );\n    }\n  )\n\n\n  return(\n    <div>\n    <Carousel activeIndex={index} onSelect={handleSelect}>\n      <Carousel.Item>\n        <img width={900} height={500}\n          className= \"d-block w-100\"\n          src=\"goPhish.jpg\"\n          alt=\"First slide\" />\n        <Carousel.Caption>\n          <h3>Go Phish!</h3>\n          <p>Can you tell which one is a scam? <a href='/GoPhish'>Click here to play</a></p>\n        </Carousel.Caption>\n      </Carousel.Item>\n      <Carousel.Item>\n        <img\n        width={900} height={500}\n          className=\"d-block w-100\"\n          src=\"ImagePlaceholder.png\"\n          alt=\"Second slide\"\n        />\n\n        <Carousel.Caption>\n          <h3>Second slide label</h3>\n          <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>\n        </Carousel.Caption>\n      </Carousel.Item>\n      <Carousel.Item>\n        <img\n        width={900} height={500}\n          className=\"d-block w-100\"\n          src=\"ImagePlaceholder.png\"\n          alt=\"Third slide\"\n        />\n\n        <Carousel.Caption>\n          <h3>Third slide label</h3>\n          <p>\n            Praesent commodo cursus magna, vel scelerisque nisl consectetur.\n          </p>\n        </Carousel.Caption>\n      </Carousel.Item>\n    </Carousel>\n    <Container>\n      <Row>\n        <Col>\n        <h2> Welcome to the Web! </h2>\n        <p> No matter your age, it's always a good practice to learn about the internet! \n          If you're a beginner, why not check out some of our resources? Or play our new GoPhish game?</p></Col>\n        <Col><h2>Resources</h2>\n        <div> {resources} </div>\n        </Col>\n\n      </Row>\n    </Container>\n    </div>\n  );\n}\n\n\nexport default Adults","/Users/regchuhi/swe250/WelcomingWeb/src/components/phishingQuiz/goPhish.js",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/components/navbar/Navbar.jsx",["105","106"],[],"import React from 'react'\nimport './navbar.css'\nimport { RiMenu3Line, RiClosedLin} from 'react-icons/ri'\n\nconst Navbar = () => {\n  return (\n    <div className='navbar'>\n      <div className='navbarlinks'>\n        <div className='navbarlinksLogo'>\n          <h1>WelcomingWeb</h1>\n        </div>\n        <div className='navbarlinkscontainer'>\n          <p><a href='/home'>Home</a></p>\n          <p><a href='/About'>About</a></p>\n          <p><a href='/Kids'>Kids</a></p>\n          <p><a href='/Adults'>Adults</a></p>\n          \n        </div>\n      </div>   \n      </div>\n  )\n}\nexport default Navbar","/Users/regchuhi/swe250/WelcomingWeb/src/components/phishingQuiz/Quiz.js",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/components/phishingQuiz/context/quiz.js",[],[],"/Users/regchuhi/swe250/WelcomingWeb/src/components/phishingQuiz/Question.js",["107"],[],"import { useContext } from \"react\";\nimport { QuizContext } from \"./context/quiz\";\nimport Answer from \"./Answer\";\n\nconst Question = () => {\n  const [quizState, dispatch] = useContext(QuizContext);\n  const currentQuestion = quizState.questions[quizState.currentQuestionIndex];\n  return (\n    <div>\n      <div className=\"question\">{currentQuestion.question}